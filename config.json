{
  "source": ["tokens.json"],
  "transform": {
    "name": "size/px",
    "type": "value",
    "matcher": "type",
    "transitive": true
  },
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "styles/",
      "files": [
        {
          "destination": "_variables.scss",
          "format": "scss/variables",
          "options": {
            "outputReferences": true
          }
        }
      ]
    }
  }
}